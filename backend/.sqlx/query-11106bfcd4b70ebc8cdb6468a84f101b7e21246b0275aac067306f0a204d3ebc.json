  "query": "\n        SELECT\n            s.yjs_state,\n            s.created_at,\n            s.modified_at,\n            s.parent_guid,\n            s.doc_type,\n            m.title,\n            m.icon,\n            m.description,\n            COALESCE(m.tags, '{}') as tags\n        FROM subdocs s\n        LEFT JOIN subdoc_metadata m ON s.guid = m.subdoc_guid\n        WHERE s.guid = $1 AND s.vault_id = $2 AND s.deleted_at IS NULL\n        ",
